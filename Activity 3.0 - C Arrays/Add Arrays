#include<stdio.h>

int main()
{
   
    int arr[10], size, i, sum = 0;
    
    printf("Enter the size of the Array:");
    scanf("%d",&size);
    if(size <= 10) {
    
    for(i = 0; i < size; i++){
    printf("Enter array elements %d:",i+1);
    scanf("%d",&arr[i]); 
    }     
         
    for(i = 0; i < size; i++)
          sum = sum + arr[i];
    
    printf("\nSum of all array = %d\n",sum);
   
    return 0;
}
     else{
          printf("Size limit is 10");
    }
    
   return 0;
}
    
